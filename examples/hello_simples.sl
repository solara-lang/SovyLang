numero idade = 25
texto nome = "Joao"
booleano ativo = verdadeiro

imprimir nome
imprimir "Idade: "
imprimir idade

se idade >= 18
    imprimir "E maior de idade"
senao
    imprimir "E menor de idade"
fim

funcao saudar(pessoa)
    imprimir "Ola, "
    imprimir pessoa
    imprimir "!"
fim

saudar(nome)

para numero i = 1 ate 5
    imprimir "Contador: "
    imprimir i
fim
